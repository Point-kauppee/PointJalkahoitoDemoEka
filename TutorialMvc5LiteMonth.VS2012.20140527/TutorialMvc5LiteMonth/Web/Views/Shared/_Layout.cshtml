<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Media/layout.css")" rel="stylesheet" type="text/css" />
</head>

<body>
        <div id="header">
			<div class="bg-help">
				<div class="inBox">
					<h1 id="logo"><a href='http://code.daypilot.org/10607/monthly-event-calendar-for-asp-net-mvc-and-jquery-open-source'>Monthly Event Calendar for ASP.NET MVC and jQuery</a></h1>
					<p id="claim"><a href="http://mvc.daypilot.org/">DayPilot for ASP.NET MVC</a> - AJAX Calendar/Scheduling Controls for ASP.NET MVC</p>
					<hr class="hidden" />
				</div>
			</div>
        </div>
        <div class="shadow"></div>
        <div class="hideSkipLink">
        </div>
        <div class="main">
            @RenderBody()
        </div>
        <div class="clear">
        </div>
</body>
</html>
